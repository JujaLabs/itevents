# Create copy of this file in same directory with name "test-local.properties" and update value of properties

# Database
database.driver=org.postgresql.Driver
database.url=jdbc:postgresql://HOST:PORT/TEST_DATABASE_NAME
database.username=DB_USERNAME
database.password=DB_PASSWORD

# Liquibase
changelog.file=src\\test\\resources\\liquibase\\db.changelog-create.xml

# EventService
event.filter.range.in.days=7
event.count.of.events.in.email=10

# JWTCryptTokenService
jwt.key=password

# UserService
user.activation.otp.lifetime.hours = 24